{
    "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
    "rushVersion": "5.85.1",
    "pnpmVersion": "6.7.1",
    "nodeSupportedVersionRange": ">=16.13.0 <17.0.0",
    "ensureConsistentVersions": true,
    "projectFolderMinDepth": 2,
    "projectFolderMaxDepth": 2,
    "repository": {
        "url": "https://github.com/askchapter/oss",
        "defaultBranch": "main",
        "defaultRemote": "origin"
    },
    "projects": [
        {
            "packageName": "rush-archive-plugin",
            "projectFolder": "packages/rush-plugins/rush-archive-plugin",
            "shouldPublish": true
        },
        {
            "packageName": "rush-monorepo-lint-plugin",
            "projectFolder": "packages/rush-plugins/rush-monorepo-lint-plugin"
        }
    ]
}
